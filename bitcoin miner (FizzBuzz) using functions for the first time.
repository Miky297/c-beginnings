#include <iostream>
#include <string>
using namespace std;
int i = 0;
int endingNum;

//-----------------------------
void func1() {
	for(i = 0; i <= endingNum; i++)
		if ( i % 3 == 0 && i % 5 == 0) {
			cout << "BitcoinMined" << endl;
		} else if( i % 3 == 0) {
			cout << " - 2" << endl;
		} else if (i % 5 == 0) {
			cout << " + 2" << endl;;
		} else {
			cout << i << "  ";
		}
}
//----------------------------
int main() {
	cout << "enter ending number here: ";
	cin >> endingNum;
	func1();

}

