#include <iostream>
#include <string>
using namespace std;
//---
//---
string myarrayNames[7] = { "Aaron", "Aaron-Jame", "Aaryn", "Aayan", "Aazaan", "Abaan", "Abbas"};
//---

//---
int age = 15;
//---
int main(){
for (int i = 0; i < 7; i++){
cout  << "name: "<< myarrayNames[i] << ", " << "age: "<< age++ << endl;

}

}
