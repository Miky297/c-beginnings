#include <iostream>
using namespace std;
int x;
int i = 1;
int main(){
cout << "enter the number you wish the program to end here: ";
cin >> x;
for( int i = 0;  i <= x; i++){
if(i % 3 == 0 && i % 5 == 0 ){
cout << "FIZZBUZZ" << endl;
} else if(i % 3 == 0 ){
cout << "fizz" << endl;
} else if (i % 5 == 0 ){
cout << "buzz" << endl;
}else {
cout << i << endl;
}
}
}
